{
  "name": "prakash-enterprises",
  "version": "1.0.0",
  "description": "Professional landing page for PRAKASH ENTERPRISES - Loans & Insurance Services",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "build:client": "cd client && npm install && npm run build",
    "build:prod": "npm run build:client && echo 'Production build completed'",
    "build:production": "node build-production.js",
    "build:netlify": "cd client && npm install && npm run build",
    "vercel-build": "node vercel-build.js",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node server.js",
    "start:prod": "set NODE_ENV=production && node server.js",
    "start:prod:unix": "NODE_ENV=production node server.js",
    "setup": "node setup.js",
    "fix-frontend": "node fix-frontend.js",
    "test-frontend": "node test-frontend.js",
    "test-compile": "node test-compile.js",
    "test-server": "node test-server.js",
    "check-dev": "node start-dev.js",
    "setup-dev": "node setup-dev.js",
    "test-all": "node test-all-apis.js",
    "deploy": "bash deploy-to-github.sh",
    "commit": "bash commit-to-github.sh",
    "start-dev": "node start-server.js",
    "add-test-data": "node add-test-visitors.js",
    "debug-auth": "node debug-auth.js",
    "test-endpoints": "node test-endpoints.js",
    "test-production": "node test-production-apis.js",
    "deploy-final": "node deploy-final.js",
    "deploy:prod": "node deploy-production.js",
    "deploy:netlify": "npm run build:netlify && echo 'Netlify build completed'",
    "vercel": "vercel --prod",
    "deploy:vercel:complete": "node deploy-vercel-complete.js",
    "test-apis": "node test-apis.js",
    "test-apis:vercel": "node test-apis.js --url https://prakash-enterprises.vercel.app",
    "test-cors": "node test-cors.js",
    "test-mongodb": "node test-mongodb.js",
    "test-admin-auth": "node test-admin-auth.js"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "axios": "^1.11.0",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "nodemailer",
    "tailwind",
    "landing-page"
  ],
  "author": "PRAKASH ENTERPRISES",
  "license": "MIT"
}
